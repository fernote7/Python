# -*- coding: utf-8 -*-
__author__ = 'Fernando'

def bissexto(n):
    if n % 4 != 0:
        a = "Ano não é bissexto"

    elif n % 4 == 0 and n % 100 != 0:
        a = "Ano é bissexto"

    elif n % 4 == 0 and n % 100 == 0 and n % 400 == 0:
        a = "Ano é bissexto"

    else:
        a = "Ano não é bissexto"

    return a



print bissexto(2011)
